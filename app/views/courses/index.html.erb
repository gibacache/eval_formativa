<%= index_title %>
<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>Sigla</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @courses.each do |course| %>
      <tr>
      <td><%= course.id %></td>
      <td><%= course.number %></td>
      <td><%= course.name %></td>
        <%= render 'shared/index_actions', object: course %>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to_new_path %>
